//digit0 is equal to digit1 then print as 1 otherwise 0
#include<stdio.h>
int main(){
    int num,digit0,digit1,digit2,digit3;
    scanf("%d",&num);
    digit0=num%10;
    digit1=(num/10)%10;
    digit2=(num/100)%10;
    digit3=num/1000;
    if(digit0==digit1){
        printf("1");
    }
    else{
        printf("0");
    }
    return 0;
}

